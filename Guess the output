1.
#include <stdio.h>
void main() 
{
int a[3]={10,'d',20.3};    
   printf("%d",a[1+1]);
}


2.
#include <stdio.h>
void main() 
{
int a[3];
{10,20,30};    
   printf("%d",a[1]);
}

3.
#include <stdio.h>
void main() 
{
int a[]={10,20,30}; 
printf("%d",2[a]);
}

4.
#include <stdio.h>
void main() 
{
int a[]={10,20,30}; 
int b[]={40,50,60}; 
a=b;
 printf("%d %d",a[0], b[0]);
}

